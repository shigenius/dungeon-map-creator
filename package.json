{
  "name": "dungeon-map-creator",
  "private": true,
  "version": "1.0.0",
  "description": "Complete 3D Dungeon RPG Map Editor - Web-based map editor with 2D grid editing and 3D preview",
  "type": "module",
  "keywords": [
    "dungeon",
    "map",
    "editor",
    "rpg",
    "3d",
    "react",
    "typescript",
    "canvas"
  ],
  "author": "Generated with Claude Code",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 100",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --reporter=default",
    "test:e2e": "npx playwright test --reporter=list",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:e2e",
    "deploy:gh": "npm run build && npx gh-pages -d dist",
    "deploy:preview": "npm run build && npm run preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.1",
    "@react-three/fiber": "^8.15.19",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/dom": "^10.4.0",
    "@types/three": "^0.159.0",
    "@types/uuid": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "three": "^0.159.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  }
}
